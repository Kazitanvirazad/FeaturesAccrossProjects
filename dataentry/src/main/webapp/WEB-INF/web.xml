<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>DataEntry</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>HomePageServlet</display-name>
		<servlet-name>HomePageServlet</servlet-name>
		<servlet-class>com.dataentry.api.controllers.HomePageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomePageServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>AddProject</display-name>
		<servlet-name>AddProject</servlet-name>
		<servlet-class>com.dataentry.api.controllers.AddProject</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddProject</servlet-name>
		<url-pattern>/addproject</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>AddFeature</display-name>
		<servlet-name>AddFeature</servlet-name>
		<servlet-class>com.dataentry.api.controllers.AddFeature</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddFeature</servlet-name>
		<url-pattern>/addfeature</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>FeatureList</display-name>
		<servlet-name>FeatureList</servlet-name>
		<servlet-class>com.dataentry.api.controllers.FeatureList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FeatureList</servlet-name>
		<url-pattern>/featurelist</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>SearchFeature</display-name>
		<servlet-name>SearchFeature</servlet-name>
		<servlet-class>com.dataentry.api.controllers.SearchFeature</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchFeature</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>ProjectList</display-name>
		<servlet-name>ProjectList</servlet-name>
		<servlet-class>com.dataentry.api.controllers.ProjectList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectList</servlet-name>
		<url-pattern>/projectlist</url-pattern>
	</servlet-mapping>


	<resource-ref>
		<description>Connection Pool</description>
		<res-ref-name>jdbc/postgres</res-ref-name>
		<res-type>javax.sql.Datasource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>