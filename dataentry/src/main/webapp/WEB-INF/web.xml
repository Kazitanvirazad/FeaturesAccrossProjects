<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>DataEntry</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>HomePageServlet</display-name>
		<servlet-name>HomePageServlet</servlet-name>
		<servlet-class>com.dataentry.api.controllers.HomePageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomePageServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>AddData</display-name>
		<servlet-name>AddData</servlet-name>
		<servlet-class>com.dataentry.api.controllers.AddData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddData</servlet-name>
		<url-pattern>/addData</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>GetListData</display-name>
		<servlet-name>GetListData</servlet-name>
		<servlet-class>com.dataentry.api.controllers.GetListData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetListData</servlet-name>
		<url-pattern>/listdata</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>SearchProjectData</display-name>
		<servlet-name>SearchProjectData</servlet-name>
		<servlet-class>com.dataentry.api.controllers.SearchProjectData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchProjectData</servlet-name>
		<url-pattern>/searchprojectdata</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>Test</display-name>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.dataentry.api.controllers.Test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<resource-ref>
		<description>Connection Pool</description>
		<res-ref-name>jdbc/postgres</res-ref-name>
		<res-type>javax.sql.Datasource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>